{
  "engines": {
    "node": "20",
    "npm": ">=9",
    "pnpm": ">=8"
  },
  "private": true,
  "scripts": {
    "dev": "npx npm-run-all --parallel dev:*",
    "dev:front": "npm run dev --workspace front",
    "dev:back": "npm run start:dev --workspace back",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspace back"
  },
  "devDependencies": {
    "prettier": "^3.1.1"
  },
  "workspaces": [
    "back",
    "front"
  ]
}
